{
  "name": "@text-adventure/text-adventure-client-api-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "text-adventure-client-api-e2e",
    "implicitDependencies": [
      "text-adventure-client-api"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "packages/text-adventure-client-api-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "text-adventure-client-api:build",
          "text-adventure-client-api:serve"
        ]
      }
    }
  }
}
